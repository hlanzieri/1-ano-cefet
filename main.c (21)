#include <stdio.h>

int main() {
    int i, j, m[5][5]={0};
    for(i=0; i<4; i++){
        for(j=0; j<4; j++){
           if(i==j){
               m[i][j]=1;
           }
            }
        }
    for(i=0; i<4; i++){
        for(j=0; j<4; j++){
            printf("%i  ", m[i][j]);
        }
        printf("\n");
    }
