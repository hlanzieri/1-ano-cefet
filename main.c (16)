#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x[3][3]={0}, i, j,soma=0;
    for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            scanf("%i", &x[i][j]);
        }
    }
    i=0;
    while(i<3){
        j=0;
            while(j<3){
                soma+=x[i][j];
                j++;
            }
        i++;
    }
    printf("\n\nsoma=>%i", soma);
    return 0;
}
