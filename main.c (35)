#include <stdio.h>

int main() {
    int A[3][3]={0}, B[3][3]={0}, i, j;
    printf("Recebendo a matriz A:\n");
    for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            scanf("%i", &A[i][j]);
            }
        }
    for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            B[i][j]=A[i][j]*A[i][j];
            printf("%i  ", B[i][j]);
            }
        printf("\n");
        }
    return 0;
}
