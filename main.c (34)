#include <stdio.h>

int main() {
    int A[3][3]={0}, B[3][3]={0}, C[3][3]={0}, i, j;
    printf("Recebendo a matriz A:\n");
    for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            scanf("%i", &A[i][j]);
            }
        }
    printf("\n\nRecebendo a matriz B:\n");
    for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            scanf("%i", &B[i][j]);
            }
        }
    for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            C[i][j]=A[i][j]*B[i][j];
            printf("%i  ", C[i][j]);
            }
        printf("\n");
        }
    return 0;
}
