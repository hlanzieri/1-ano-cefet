#include <stdio.h>

int main() {
    int i, j, m[3][3]={0}, n[3][3]={0};
    for(i=0; i<3; i++){
        for(j=0; j<3; j++){
            scanf("%i", &m[i][j]);
            n[j][i]=m[i][j];
            }
        }
        for(i=0; i<3; i++){
            for(j=0; j<3; j++){
            printf("%i  ", n[i][j]);
            }
            printf("\n");
        }
    return 0;
}
